{
  "name": "scheduler-75fe",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "build:e2e": "cross-env NODE_ENV=test remix build",
    "dev": "cross-env PORT=3000 remix dev --manual -c \"arc sandbox -e testing\"",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start:e2e": "cross-env NODE_ENV=test PORT=3000 arc sandbox",
    "test:e2e": "npm run test:e2e:prepare && playwright test && lsof -t -i:3000 && kill -9 $( lsof -i:3000 -t )",
    "test:e2e:visualize": "npm run test:e2e:prepare && PWDEBUG=1 playwright test --headed && lsof -t -i:3000 && kill -9 $( lsof -i:3000 -t )",
    "test:e2e:prepare": "./scripts/setup-playwright-test.sh",
    "test:e2e:install": "npx playwright install chromium --with-deps",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test",
    "zapatos": "node -r dotenv/config zapatos.js"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/server/index.js",
    "/public/build"
  ],
